{
    "name": "gouguoyin/laravel-log-viewer",
    "description": "Laravel Log Viewer 为laravel提供了一个基于bootstrap搭建，完美适配PC、平板和移动端的日志查看后台，可自由配置访问路由、权限策略、中间件、导航链接，支持多语言和日志搜索、下载、删除",
    "license": "MIT",
    "keywords": ["log-viewer", "log-reader", "logs-viewer", "logs-reader", "laravel-log-viewer", "laravel-log-reader", "laravel-logs-viewer", "laravel-logs-reader", "laravel"],
    "type": "laravel-package",
    "authors": [
        {
            "name": "gouguoyin",
            "email": "245629560@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "4.2.*|5.*|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Gouguoyin\\LogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gouguoyin\\LogViewer\\LogViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
